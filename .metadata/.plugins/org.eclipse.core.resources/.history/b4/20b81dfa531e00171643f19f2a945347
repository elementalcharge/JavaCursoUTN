public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		CuentaControlador cc = new CuentaControlador();
		cc.obtenerSaldo();
		cc.retiroDinero(122);
		cc.acreditoDinero(2000);
		cc.obtenerSaldo();
		cc.retiroDineroCC(3000);
		cc.retiroDineroCC(4000);
		cc.obtenerSaldoCC();
		cc.chequeoSaldoCC();
		
